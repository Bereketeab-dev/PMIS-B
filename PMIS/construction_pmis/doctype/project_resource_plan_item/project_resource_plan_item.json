            "default": "1",
            "in_list_view": 1
        },
        {
            "fieldname": "start_date",
            "label": "Planned Start Date",
            "fieldtype": "Date"
        },
        {
            "fieldname": "end_date",
            "label": "Planned End Date",
            "fieldtype": "Date"
        },
        {
            "fieldname": "notes",
            "label": "Notes",
            "fieldtype": "Small Text"
